      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.1.4",
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
